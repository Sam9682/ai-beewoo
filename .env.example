# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/beewoo
DB_POOL_SIZE=10

# Web2py Configuration
WEB2PY_PASSWORD=your_secure_password_here
WEB2PY_ADMIN_SECURITY_BYPASS=false
SECRET_KEY=your_secret_key_here

# Security Settings
SSL_CERT_PATH=/path/to/cert.pem
SSL_KEY_PATH=/path/to/key.pem
ALLOWED_HOSTS=localhost,127.0.0.1

# OTHSEC Client Configuration
OTHSEC_PORT=20508
OTHSEC_INTERFACE=eth0
OTHSEC_SSL_ENABLED=true

# External Services
IPLIGENCE_API_KEY=your_api_key
REDIS_URL=redis://localhost:6379/0

# Logging
LOG_LEVEL=INFO
LOG_FILE=/var/log/beewoo/app.log